//Example 3: Checking if a character is a vowel or consonant

#include <stdio.h>

int main() {
    char ch;
    printf("Enter a character: ");
    scanf(" %c", &ch);

    if (ch == 'a' || ch == 'e' || ch == 'i' || ch == 'o' || ch == 'u' ||
        ch == 'A' || ch == 'E' || ch == 'I' || ch == 'O' || ch == 'U') {
        printf("It's a vowel.\n");
    } else {
        printf("It's a consonant.\n");
    }

    return 0;
}
