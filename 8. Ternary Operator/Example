// Example 1: Checking if a number is positive or negative using the ternary operator

#include<stdio.h>

int main()
{
  int num;
  printf("Enter a Number: ");
  scanf("%d", &num);
  // condition ? expression1 : expression2

  num >= 0 ? printf("Positive Number\n") : printf("Negative Number\n");
  return 0;
}
